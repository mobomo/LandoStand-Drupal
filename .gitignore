# Ignore directories generated by Composer
vendor
webroot/core
webroot/libraries
webroot/modules/contrib
webroot/themes/contrib
webroot/profiles/contrib

# Ignore Drupal's file directory
webroot/sites/*/files

# Ignore files generated by JetBrains and other IDEs
.idea
.vscode

# Ignore export files and other things that probably shouldn't be committed.
*.tar
salt.txt
.env
*.sql

# Ignore compiled app CSS files
webroot/themes/custom/**/**/*.css

# Ignore file logs and other vendor files
logs
node_modules
console
/.editorconfig

# Ignore secrets
.secrets.env

# Ignore local override file for phpstan
phpstan.neon
